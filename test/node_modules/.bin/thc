#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/eric/projects/hmr/test-hmr/cli.js/node_modules:/home/eric/projects/hmr/test-hmr/node_modules:/home/eric/projects/hmr/node_modules:/home/eric/projects/node_modules:/home/eric/node_modules:/home/node_modules:/node_modules:/home/eric/.nvm/versions/node/v12.16.3/pnpm-global/node_modules/test-hmr/cli.js/node_modules:/home/eric/.nvm/versions/node/v12.16.3/pnpm-global/node_modules/test-hmr/node_modules:/home/eric/.nvm/versions/node/v12.16.3/pnpm-global/node_modules:/home/eric/.nvm/versions/node/v12.16.3/node_modules:/home/eric/.nvm/versions/node/node_modules:/home/eric/.nvm/versions/node_modules:/home/eric/.nvm/node_modules:/home/eric/projects/svench/svench/test/node_modules:/home/eric/projects/svench/svench/node_modules:/home/eric/projects/svench/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../.nvm/versions/node/v12.16.3/pnpm-global/node_modules/test-hmr/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../../.nvm/versions/node/v12.16.3/pnpm-global/node_modules/test-hmr/cli.js" "$@"
fi
